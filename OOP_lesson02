
int[] nums =new int[8];
void setup() {
  size(500, 400);

  for (int i=0; i<nums.length; i++) {
    nums[i]=int(random(10, 100));
  };
};
void draw() {
  background(0);
  noFill();
  stroke(255);
  strokeWeight(5);
  
  for (int i=0; i<nums.length; i++) {
    ellipse(i*50+100, 200, nums[i], nums[i]);
  };
};
