int y;
float x=0;
int speed =5;
float theta = 0;
void setup() {
  size(1080, 900);
  frameRate(70);
}
void draw() {

  translate(width/2, height/2);
  rotate(theta);
  background(100);
  y+=speed;
   for (int i= 0; i<10; i+=10) {
    text("word", i, y*1.5); 
      
  theta += radians(1);
  };
    if (y>height) {
    y=-1;
  };
};
