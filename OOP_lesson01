//lesson about arrays



float[] nums ={30, 40, 92, 50};//index is the address of the array, [0][1][2][3]
void setup() {
  size(500, 400);
};
void draw() {
  background(0);
  fill(255);
  //ellipse(100, 50, num, num);


  for (int i=0; i<nums.length; i++) {
    ellipse(i*50+100, 200, nums[i], nums[i]);
  };
};
